{
    "name": "javier/Boilerplate",
    "description": "Package Laravel to test publication with none function",
    "keywords": [
        "javier",
        "Boilerplate"
    ],
    "homepage": "https://github.com/javiersolis/laravel-publish-test-package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Javier Solis",
            "email": "solis.unmsm@gmail.com",
            "role": "senior"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "naoray/laravel-package-maker": "^4.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "javier\\Boilerplate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "javier\\Boilerplate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "javier\\Boilerplate\\BoilerplateServiceProvider"
            ],
            "aliases": {
                "LaravelPublishTestPackage": "javier\\Boilerplate\\BoilerplateFacade"
            }
        }
    }
}
