{
    "name": "javierboilerplate/JavierBoilerplate",
    "description": "Package Laravel to test publication with none function",
    "keywords": [
        "javierboilerplate",
        "JavierBoilerplate"
    ],
    "homepage": "https://github.com/javiersolis/javier-boilerplate",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Javier Solis",
            "email": "none@none.com",
            "role": "creator"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "naoray/laravel-package-maker": "^4.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "javierboilerplate\\JavierBoilerplate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "javierboilerplate\\JavierBoilerplate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "javierboilerplate\\JavierBoilerplate\\JavierBoilerplateServiceProvider"
            ],
            "aliases": {
                "LaravelPublishTestPackage": "javierboilerplate\\JavierBoilerplate\\JavierBoilerplateFacade"
            }
        }
    }
}
